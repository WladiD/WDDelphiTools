<NotepadPlus>
    <UserLang name="DptAiWorkflow" ext="DptAiWorkflow" udlVersion="2.1">
        <Settings>
            <Global caseIgnored="yes" allowFoldOfComments="no" foldCompact="no" forcePureLC="0" decimalSeparator="0" />
            <Prefix Keywords1="no" Keywords2="no" Keywords3="no" Keywords4="no" Keywords5="no" Keywords6="no" Keywords7="no" Keywords8="no" />
        </Settings>
        <KeywordLists>
            <Keywords name="Comments">00#</Keywords>
            <Keywords name="Numbers, prefix1"></Keywords>
            <Keywords name="Numbers, prefix2"></Keywords>
            <Keywords name="Numbers, extras1"></Keywords>
            <Keywords name="Numbers, extras2"></Keywords>
            <Keywords name="Numbers, suffix1"></Keywords>
            <Keywords name="Numbers, suffix2"></Keywords>
            <Keywords name="Numbers, range"></Keywords>
            <Keywords name="Operators1">: ( ) &gt; &lt; = ! + - * /</Keywords>
            <Keywords name="Operators2"></Keywords>
            <Keywords name="Folders in code1, open">{</Keywords>
            <Keywords name="Folders in code1, middle"></Keywords>
            <Keywords name="Folders in code1, close">}</Keywords>
            <Keywords name="Folders in code2, open"></Keywords>
            <Keywords name="Folders in code2, middle"></Keywords>
            <Keywords name="Folders in code2, close"></Keywords>
            <Keywords name="Folders in comment, open"></Keywords>
            <Keywords name="Folders in comment, middle"></Keywords>
            <Keywords name="Folders in comment, close"></Keywords>
            <Keywords name="Keywords1">BeforeDptGuard AfterDptGuard</Keywords>
            <Keywords name="Keywords2">and or not</Keywords>
            <Keywords name="Keywords3">AiSessionStarted IsCurrentAction IsCurrentAiSessionAction GetCurrentLintTargetFile GetCurrentProjectFiles IsFileRegisteredInAiSession HasValidLintResult RequestDptExit RequestDptExitWithCode GetExitCode IsCurrentBuildProjectFile GetInvalidLintFiles</Keywords>
            <Keywords name="Keywords4">True False</Keywords>
            <Keywords name="Keywords5"></Keywords>
            <Keywords name="Keywords6"></Keywords>
            <Keywords name="Keywords7"></Keywords>
            <Keywords name="Keywords8"></Keywords>
            <Keywords name="Delimiters">00" 01 02" 03` 04 05`</Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" styleID="11" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="COMMENTS" styleID="1" fgColor="008000" bgColor="FFFFFF" fontStyle="2" />
            <WordsStyle name="LINE COMMENTS" styleID="2" fgColor="008000" bgColor="FFFFFF" fontStyle="2" />
            <WordsStyle name="NUMBERS" styleID="4" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="KEYWORDS1" styleID="5" fgColor="0000FF" bgColor="FFFFFF" fontStyle="1" />
            <WordsStyle name="KEYWORDS2" styleID="6" fgColor="FF00FF" bgColor="FFFFFF" fontStyle="1" />
            <WordsStyle name="KEYWORDS3" styleID="7" fgColor="008080" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="KEYWORDS4" styleID="8" fgColor="000080" bgColor="FFFFFF" fontStyle="1" />
            <WordsStyle name="KEYWORDS5" styleID="9" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="KEYWORDS6" styleID="10" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="KEYWORDS7" styleID="11" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="KEYWORDS8" styleID="12" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="OPERATORS" styleID="10" fgColor="0000FF" bgColor="FFFFFF" fontStyle="1" />
            <WordsStyle name="FOLDER IN CODE1" styleID="12" fgColor="0000FF" bgColor="FFFFFF" fontStyle="1" />
            <WordsStyle name="FOLDER IN CODE2" styleID="13" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="FOLDER IN COMMENT" styleID="14" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="DELIMITERS1" styleID="14" fgColor="804000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="DELIMITERS2" styleID="15" fgColor="0000FF" bgColor="E0E0E0" fontStyle="0" />
            <WordsStyle name="DELIMITERS3" styleID="16" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="DELIMITERS4" styleID="17" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="DELIMITERS5" styleID="18" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="DELIMITERS6" styleID="19" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="DELIMITERS7" styleID="20" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="DELIMITERS8" styleID="21" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
        </Styles>
    </UserLang>
</NotepadPlus>
